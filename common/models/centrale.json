{
  "name": "centrale",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nature": {
      "type": "string",
      "required": true
    },
    "capacite": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "energie": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "",
      "options": {
        "nestRemoting": true,
        "disableInclude": false
      }
    },
    "historiques": {
      "type": "hasMany",
      "model": "historique",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      },
      "through": "historique"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
